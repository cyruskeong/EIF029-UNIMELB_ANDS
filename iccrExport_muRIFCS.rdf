   <rdf:Description rdf:about="dcExport://dc3central/International_Centre_for_Classroom_Researchiccr:countryCollection_Learner's_Perspective_Study:_An_International_Research_Collaboration_in_Mathematics_Education_Germany">
      <ands:harvestedBy xmlns:ands="http://www.ands.org.au/ontologies/ns/0.1/VITRO-ANDS.owl#"
