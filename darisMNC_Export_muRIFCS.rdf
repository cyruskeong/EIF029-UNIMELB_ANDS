   <rdf:Description rdf:about="dcExport://dc3central/DaRIS_1047___Melbourne_Neuropsychiatry_Centre,_Department_of_Psychiatrydaris:project_id:1047_2_2">
      <ands:harvestedBy xmlns:ands="http://www.ands.org.au/ontologies/ns/0.1/VITRO-ANDS.owl#"
                        rdf:resource="https://rdr.unimelb.edu.au/vivo/n5820"/>
